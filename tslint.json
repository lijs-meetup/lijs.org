{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-immutable"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "interface-name": [false],
      "jsx-no-multiline-js": [false],
      "object-literal-sort-keys": false,
      "no-require-imports": false,
      "no-var-requires": false,
      "arrow-parens": false
    },
    "rulesDirectory": []
}